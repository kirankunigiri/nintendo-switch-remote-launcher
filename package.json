{
	"name": "switch-remote-launcher",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"dev": "node dev.js",
		"build": "remix vite:build && rm -rf romfs && esbuild --bundle --sourcemap --sources-content=false --target=es2022 --define:DEV_MODE=false --format=esm src/main.ts --outfile=romfs/main.js && cp -r build/client romfs/public",
		"build:dev": "remix vite:build && rm -rf romfs && esbuild --bundle --sourcemap --sources-content=false --target=es2022 --define:DEV_MODE=true --format=esm src/main.ts --outfile=romfs/main.js && cp -r build/client romfs/public",
		"nro": "nxjs-nro",
		"nsp": "nxjs-nsp",
		"start": "remix-serve ./build/server/index.js",
		"build:client": "remix vite:build",
		"dev:web": "remix vite:dev"
	},
	"dependencies": {
		"@legendapp/state": "^3.0.0-beta.30",
		"@mantine/core": "^7.17.3",
		"@mantine/hooks": "^7.17.3",
		"@nx.js/http": "^0.0.7",
		"@remix-run/node": "^2.16.3",
		"@remix-run/react": "^2.16.3",
		"@remix-run/serve": "^2.16.3",
		"@remix-run/server-runtime": "^2.8.0",
		"@tootallnate/nacp": "^0.0.1",
		"buffer": "^6.0.3",
		"cors": "^2.8.5",
		"express": "^4.18.3",
		"isbot": "^4.1.0",
		"nintendo-switch-eshop": "^8.0.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-icons": "^5.5.0",
		"remix-utils": "^8.5.0",
		"serve-static": "^1.15.0"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.2",
		"@eslint/js": "^9.14.0",
		"@nx.js/nro": "^0.0.63",
		"@nx.js/nsp": "^0.0.63",
		"@remix-run/dev": "^2.16.3",
		"@stylistic/eslint-plugin": "^2.10.1",
		"@types/eslint__js": "^8.42.3",
		"@types/react": "^18.2.20",
		"@types/react-dom": "^18.2.7",
		"@typescript-eslint/parser": "^6.7.4",
		"autoprefixer": "^10.4.19",
		"chokidar": "^3.6.0",
		"esbuild": "^0.17.19",
		"eslint": "^9.14.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-jsonc": "^2.20.0",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"eslint-plugin-tailwindcss": "^3.17.5",
		"globals": "^15.13.0",
		"nxjs-runtime": "^0.0.50",
		"postcss": "^8.4.38",
		"tailwindcss": "^3.4.4",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.13.0",
		"vite": "^6.0.0",
		"vite-tsconfig-paths": "^4.2.1"
	}
}